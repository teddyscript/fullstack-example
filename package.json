    "asset-require-hook": "^1.2.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "commander": "^4.0.1",
    "compression": "^1.7.4",
    "connected-react-router": "^6.5.2",
    "cors": "^2.8.5",
    "css-modules-require-hook": "^4.2.3",
    "dotenv-flow": "^3.1.0",
    "emojione": "^4.5.0",
